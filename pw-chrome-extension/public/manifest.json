{
  "manifest_version": 2,
  "name": "PennyWise Chrome Extension",
  "author": "PennyWise",
  "version": "1.0.1",
  "description": "Allow users to save items to their PennyWise Wish List Dashboard.",
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "",
      "32": "",
      "48": "",
      "128": ""
    }
  },
  "icons": {
    "16": "",
    "48": "",
    "128": ""
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "PennyWise"
  },
  "content_security_policy": "script-src 'self' 'sha256-GgRxr...'; object-src 'self'",
  "permissions": [
    "storage",
    "tabs",
    "declarativeContent"
  ],
  "web_accessible_resources": [
    "*.html"
  ]
}