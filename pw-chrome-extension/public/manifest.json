{
  "manifest_version": 2,
  "name": "PennyWise Chrome Extension",
  "author": "PennyWise",
  "version": "1.0.1",
  "description": "Allow users to save items to their PennyWise Wish List Dashboard.",
  "icons": {
    "16": "",
    "48": "",
    "128": ""
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "PennyWise"
  },
  "content_security_policy": "script-src 'self' 'sha256-GgRxr...'; object-src 'self'",
  "permissions": [
    "storage",
    "contextMenus",
    "tabs"
  ],
  "web_accessible_resources": [
    "*.html"
  ]
}